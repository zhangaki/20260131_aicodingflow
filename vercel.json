{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "astro",
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/blog/editor-wars",
            "destination": "/blog",
            "permanent": true
        }
    ]
}